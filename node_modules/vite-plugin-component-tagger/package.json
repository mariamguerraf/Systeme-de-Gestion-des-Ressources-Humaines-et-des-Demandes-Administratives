{
  "name": "vite-plugin-component-tagger",
  "version": "0.2.6",
  "description": "A Vite plugin for tagging components with metadata attributes for debugging and development",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "keywords": [
    "vite",
    "vite-plugin",
    "component",
    "tagger"
  ],
  "author": "Didier Catz <catz@duck.com>",
  "license": "MIT",
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.7",
    "@types/node": "^20.10.5",
    "@vitest/ui": "^3.1.3",
    "bits-ui": "^1.4.8",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^4.2.8",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.0",
    "vitest": "^1.0.4"
  },
  "peerDependencies": {
    "vite": ">=5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "example:svelte": "vite example/svelte --config example/vite.config.js",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint . --ext .js,.ts"
  }
}